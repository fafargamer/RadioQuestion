<head>
    <meta charset="utf-8">
  
    <title>The HTML5 Herald</title>
    <meta name="description" content="The HTML5 Herald">
    <meta name="author" content="SitePoint">
  
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">
    <link href="/css/style.css" rel="stylesheet" type="text/css" crossorigin>
  
  </head>

  <body>
    <div class="container-fluid mt-5" style="max-width: 1920px; width: 100%;">
        <div class="d-flex justify-content-center row question bg-white p-3 border-bottom">

        <form class="question bg-white p-3 border-bottom" action="/addSubParameter" method="POST"  style="width: 800px;">
          <div class="d-flex flex-row align-items-center question-title justify-content-center">
            <!-- <h3 class="text-danger">Q.</h3> -->
            <h2 class="">Tambah Sub-Parameter</h2>     
          </div> 
          <div>
            <h1>

            </h1>
          </div>
          <div class="d-flex flex-row align-items-center question-title"> 
            <h6 class="">Aspek : <%=idAspek%></h6>    
          </div>
          <div class="d-flex flex-row align-items-center question-title"> 
            <h6 class="">Indikator : <%=idIndikator%></h6>    
          </div>
          <div class="d-flex flex-row align-items-center question-title"> 
            <h6 class="">Parameter : <%=parameterID%></h6>  
          </div>
          <div class="row top-buffer">
            <h1>
              
            </h1>
          </div>   

          <% if (errMsg.length > 0) { %>
            <div class="alert alert-danger" role="alert">
              <%=errMsg%>
            </div>
  
            <% }; %>
            <div class="form-group">
                <label for="inputParameter">Sub-Parameter</label>
                <input type="text" class="form-control" id="inputSubParameter" name="inputSubParameter" placeholder="">
            </div>
            <div class="form-group">
              <label for="inputParameter">Index Sub-Parameter</label><span>
              <input type="text" class="form-control" id="inputIndex" name="inputIndex" placeholder="">
          </div>
            <div class="form-group">
              <input type="text" class="form-control" id="inputParameter" name="inputParameter" value="<%=parameterID%>" hidden>
            </div>
            <div class="form-group">
              <input type="text" class="form-control" id="inputAspek" name="inputAspek" value="<%=idAspek%>" hidden>
            </div>
            <div class="form-group">
              <input type="text" class="form-control" id="inputIndikator" name="inputIndikator" value="<%=idIndikator%>" hidden>
            </div>
            <a class="btn btn-danger" href="/<%=idAspek%>/<%=idIndikator%>/<%=parameterID%>/" role="button" style="float: left;">Back</a> 
            <button type="submit" class="btn btn-primary" style="float: right;">Submit</button>
        </form>
        </div>
    </div>


  </body>