<head>
    <meta charset="utf-8">
  
    <title>The HTML5 Herald</title>
    <meta name="description" content="The HTML5 Herald">
    <meta name="author" content="SitePoint">
  
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">
    <link href="/css/style.css" rel="stylesheet" type="text/css" crossorigin>
  
  </head>

  <body>
    <div class="container-fluid mt-5" style="max-width: 1920px; width: 100%;">
      <div class="d-flex justify-content-center row question bg-white p-3 border-bottom">

        <div class="d-flex justify-content-center row">
        <form action="/addParameter" method="POST"  style="width: 800px;">
          <div class="d-flex flex-row align-items-center question-title justify-content-center">
            <!-- <h3 class="text-danger">Q.</h3> -->
            <h2 class="">Tambah Parameter</h2>     
          </div>         
          <div>
            <h1>

            </h1>
          </div>
          <div class="d-flex flex-row align-items-center question-title"> 
            <h6 class="">Aspek : <%=idAspek%></h6>    
          </div>
          <div class="d-flex flex-row align-items-center question-title"> 
            <h6 class="">Indikator : <%=idIndikator%></h6>    
          </div>
          <div class="row top-buffer">
            <h1>
              
            </h1>
          </div>   
          
          <% if (errMsg.length > 0) { %>
            <div class="alert alert-danger" role="alert">
              <%=errMsg%>
            </div>
  
            <% }; %>


            <div class="form-group">
                <label for="inputParameter">Parameter</label>
                <input type="text" class="form-control" id="inputParameter" name="inputParameter" placeholder="Mohon input parameter">
            </div>
            <div class="form-group">
              <label for="inputParameter">Nomor Parameter</label>
              <input type="text" class="form-control" id="inputNoParameter" name="inputNoParameter" placeholder="Mohon input nomor parameter">
            </div>         
            <div class="form-group">
              <input type="text" class="form-control" id="inputIndikator" name="inputIndikator" value="<%=idIndikator%>" hidden readonly>
            </div>     
            <div class="form-group">
              <input type="text" class="form-control" id="inputAspek" name="inputAspek" value="<%=idAspek%>" hidden readonly>
            </div>        
            <div class="form-group">
                <label for="exampleInputEmail1">Bobot</label>
                <input type="text" class="form-control" id="inputBobot" name="inputBobot" placeholder="Mohon input Bobot">
            </div>
            <a class="btn btn-danger" href="/<%=idAspek%>/<%=idIndikator%>/" role="button" style="float: left;">Back</a>              
            <button type="submit" class="btn btn-primary" style="float: right;">Submit</button>
        </form>
        </div>
      </div>
    </div>


  </body>