                    </div>
                </div>
            </div>

            <div class="main-content-inner">
                <div class="row">
                    <div class="col-12 mt-5">
                        <div class="card">
                            <div class="card-body">
                                <h2>Daftar Parameter</h2>
                                <a class="btn btn-success" href="/GCGData/<%=idAspek%>/<%=idIndikator%>/addParameter" role="button" style="float: right;">+</a>
                                <table class="table table-condensed" id="tabel">
                                    <thead style="height: 100px; background-color: lightskyblue;">
                                        <tr>
                                            <th>No</th>
                                            <th>Parameter</th>
                                            <th>Jumlah Sub-Parameter</th>                    
                                            <th>Bobot</th>
                                            <th>Nilai</th>
                                            <th> </th>
                                            <th> </th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <% result.forEach(function(row, i) { %> 
                                        <tr>
                                            <td><%=row.index%></td>
                                            <td><%=row.pertanyaan%></td>
                                            <td><%=row.jumlahSubParameter%></td>
                                            <td><%=row.bobot%></td>
                                            <td><%=row.nilai%></td>
                                            <td>
                                                <a class="btn btn-primary" href="/GCGData/<%=idAspek%>/<%=idIndikator%>/<%=row.IDPertanyaan%>/" role="button" style="text-align: center;">Detail</a>
                                            </td>
                                            <td class="justify-content-center">
                                                <!-- Button trigger modal -->
                                              <button type="button" class="btn btn-danger" data-toggle="modal" data-target="#modalAspek<%=i+1%>">
                                                Hapus
                                              </button>
                        
                                              <!-- Modal -->
                                              <div class="modal fade" id="modalAspek<%=i+1%>" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
                                                <div class="modal-dialog" role="document">
                                                  <div class="modal-content">
                                                    <div class="modal-header">
                                                      <h5 class="modal-title" id="exampleModalLabel">Yakin hapus data berikut:</h5>
                                                      <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                                        <span aria-hidden="true">&times;</span>
                                                      </button>
                                                    </div>
                                                    <div class="modal-body">
                                                        Parameter <%=row.index%> : <%=row.pertanyaan%>
                                                    </div>
                                                    <div class="modal-footer">
                                                      <a type="button" class="btn btn-secondary" data-dismiss="modal">Close</a>
                                                      <a type="button" class="btn btn-danger" href="/GCGData/<%=idAspek%>/<%=idIndikator%>/hapusParameter/<%=row.IDPertanyaan%>/">Hapus data</a>
                                                    </div>
                                                  </div>
                                                </div>
                                              </div>
                                            </td>
                                        </tr>
                                        <% }); %> 
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>

