                                      </div> -->
                                      <div class="row top-buffer">
                                        <h1>
                                          
                                        </h1>
                                      </div>  
                            
                                      <% if (message.length > 0) { %>
                                        <div class="alert alert-danger" role="alert">
                                          <%=message%>
                                        </div>
                              
                                        <% }; %>
                            
                            
                                        <div class="form-group" hidden>
                                            <label for="inputFaktor">Original Username</label>
                                            <input type="text" class="form-control" id="inputOrigUsername" name="inputOrigUsername" value="<%=data.username%>" required hidden readonly>
                                        </div>
                                        <div class="form-group">
                                            <label for="inputFaktor">Username</label>
                                            <input type="text" class="form-control" id="inputUsername" name="inputUsername" value="<%=data.username%>" required>
                                        </div>
                                        <div class="form-group">
                                          <label for="inputIndex">Password</label><span>
                                          <input type="text" class="form-control" id="inputPassword" name="inputPassword" value="<%=data.password%>" required>
                                      </div>
                                      <div class="form-group">
                                        <label for="inputIndex">Confirm Password</label><span>
                                        <input type="text" class="form-control" id="inputConfPass" name="inputConfPass" value="<%=data.password%>" required>
                                        </div>
                                        <div class="form-group">
                                            <label for="inputRole">Role</label>
                                            <select class="form-control" name="inputRole" id="inputRole"  style="height: 40px;">
                                                <option selected disabled value="0">Pilih role user</option>
                                                <option value="User">User</option>
                                                <option value="Super-user">Super-user</option>
                                                <option value="Admin">Admin</option>
                                            </select>
                                        </div>
                                        <div class="form-group">
                                            <label for="inputAspekUser">Aspek yang dapat diisi</label>
                                            <select class="form-control" name="inputAspekUser" id="inputAspekUser" style="height: 40px;">
                                                <option selected disabled>Pilih aspek user</option>
                                                <option value="1">1</option>
                                                <option value="2">2</option>
                                                <option value="3">3</option>
                                                <option value="4">4</option>
                                                <option value="5">5</option>
                                                <option value="6">6</option>
                                                <option value="7">7</option>
                                                <option value="99" hidden>-</option>                                                                                        
                                            </select>
                                        </div>
                                        <a class="btn btn-danger" href="/UserManagement/" role="button" style="float: left;">Back</a>
                                        <button type="submit" class="btn btn-primary" style="float: right;">Ubah</button>
                                    </form>
