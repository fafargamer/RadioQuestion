    <script>
        $(document).ready(function() {
            $('#tabel').DataTable({
                "lengthChange": false,
                "paginate": false,
                "info": false,
                "columnDefs": [
                    { "width": "40%", "targets": 1 }
                ]
            });

            $(function(){
                $('[name="inputRole"]').val(['<%=data.typeUser%>'])
                $('[name="inputAspekUser"]').val(['<%=data.aspekUser%>'])
                $("#inputRole").change(function(){
                    $("#inputAspekUser").val=('', $(this).val() != 'User');
                    $("#inputAspekUser").prop('disabled', $(this).val() != 'User');
                });
            });
        } );
