    <script>
        // collapseItem = localStorage.getItem('collapseItem'); 
        $(document).ready(function() {
            // var MY_ARRAY= 'my_array';
            // var arr=[1,2,3,4,5];
            // localStorage.setItem(MY_ARRAY, JSON.stringify(arr));
            if (localStorage.getItem('collapsedItems') === null) {
                Collapses = [0]
                // console.log(Collapses)
            }
            else {
                Collapses = JSON.parse(localStorage.getItem('collapsedItems'));
                // console.log(Collapses)
            }

            for (i = 0; i < Collapses.length; ++i) {
                $(Collapses[i]).collapse('show')
            }
            // console.log(collapseItem)


            $('#tabel').DataTable({
                "lengthChange": false,
                "paginate": false,
                "info": false,
                "columnDefs": [
                    { "width": "40%", "targets": 1 }
                ]
            });

            $("td.nilaiOverall").each(function() {
                $(this).html() < 15 ? $(this).css('background-color', '#d64161') : null;

                ($(this).html() >= 15 && $(this).html() < 45) ? $(this).css('background-color', '#fee227'): null;

                ($(this).html() >= 45 && $(this).html() < 90) ? $(this).css('background-color', '#9ccc9c'): null;

                $(this).html() >= 90 ? $(this).css('background-color', '#0e6b0e') : null;

            });

            $('td.validationData').each(function(){
                console.log($(this).html())
                var tr=$(this).closest('tr');
                    
                    if ($(this).html()!=1) {  
                        
                        tr.css('background-color', '#BBBBBB')
                    }     
                });
                     

            $('.collapse').on('hidden.bs.collapse', function () {
                // read the data-default value
                var defaultDiv = $($(this).data("parent")).data("default");
                // show the default panel
                $('.collapse').eq(defaultDiv-1).collapse('show');
            })

            $('.formSubFaktor').each(function(){
                
                var FileTypes = $('#'+$(this).attr('id') +' '+'#iBuktiPemenuhan').val();
                var nilai = $('#'+$(this).attr('id') + ' ' +  '#iIsiNilai').val();
                var validasi = $('#'+$(this).attr('id') + ' ' +  '#iIsiValid').val()
                console.log(nilai)
                console.log(validasi)
                // var stringQuery = '#'+$(this).attr('id')
                // console.log($(this).html())
                    
                
                if(FileTypes.includes('O')==true){
                    $('#'+$(this).attr('id') + ' ' +  '#inputPemenuhanO').attr("checked", "checked");
                    // $("#postForm #inputPemenuhanO").attr("checked", "checked");
                }    
                if(FileTypes.includes('D')==true){
                    $('#'+$(this).attr('id') + ' ' +  '#inputPemenuhanW').attr("checked", "checked");
                    // $("#postForm #inputPemenuhanO").attr("checked", "checked");
                } 
                if(FileTypes.includes('K')==true){
                    $('#'+$(this).attr('id') + ' ' +  '#inputPemenuhanK').attr("checked", "checked");
                    // $("#postForm #inputPemenuhanO").attr("checked", "checked");
                } 
                if(FileTypes.includes('W')==true){
                    $('#'+$(this).attr('id') + ' ' +  '#inputPemenuhanW').attr("checked", "checked");
                    // $("#postForm #inputPemenuhanO").attr("checked", "checked");
                } 
                
                // $(this).children('input[name="inputNilai"]').val(nilai)
                // $('#'+$(this).attr('id') + ' ' +  '#inputNilai[value='+nilai+']').prop('checked', true);
                var string = '#'+$(this).attr('id') + ' ' +  '#inputValid-'+validasi
                var string2 = '#'+$(this).attr('id') + ' ' +  '#inputNilai-'+nilai
                console.log(string2)
                console.log(string)
                if(nilai == 0) {
                    $('#'+$(this).attr('id') + ' ' +  '#inputNilaiA').attr('checked', true);
                }
                else if(nilai == 0.25) {
                    $('#'+$(this).attr('id') + ' ' +  '#inputNilaiB').attr('checked', true);
                }
                else if(nilai == 0.5) {
                    $('#'+$(this).attr('id') + ' ' +  '#inputNilaiC').attr('checked', true);
                }
                else if(nilai == 0.75) {
                    $('#'+$(this).attr('id') + ' ' +  '#inputNilaiD').attr('checked', true);
                }
                else if(nilai == 1.0) {
                    $('#'+$(this).attr('id') + ' ' +  '#inputNilaiE').attr('checked', true);
                }
                // $('#'+$(this).attr('id') + ' ' +  '#inputNilai-'+nilai).attr('checked', true);
                $('#'+$(this).attr('id') + ' ' +  '#inputValid-'+validasi).attr('checked', true);
            });


            $('.aspek-collapse').on('click', function(event) {
                if(Collapses.includes($(this).attr('href'))){
                    Collapses = []
                    console.log(Collapses)
                }
                else {
                    Collapses = []
                    console.log($(this).attr('href'))
                    Collapses.push($(this).attr('href'));
                    console.log(Collapses)
                }
                localStorage.setItem('collapsedItems', JSON.stringify(Collapses));
            })

            $('.indikator-collapse').on('click', function(event) {
                if(Collapses.includes($(this).attr('href'))){
                    Collapses[1] = ''
                    console.log(Collapses)
                }
                else {
                    if(Collapses.length > 1){
                    Collapses.splice(1, 1, $(this).attr('href'));
                }
                else {
                    console.log($(this).attr('href'))
                    Collapses.push($(this).attr('href'));
                }
                }
                console.log(Collapses)
                localStorage.setItem('collapsedItems', JSON.stringify(Collapses));
            })

            $('.parameter-collapse').on('click', function(event) {
                if(Collapses.includes($(this).attr('href'))){
                    Collapses[2] = ''
                    console.log(Collapses)
                }
                else {
                    if(Collapses.length > 2){
                    Collapses.splice(2, 1, $(this).attr('href'));
                }
                else {
                    console.log($(this).attr('href'))
                    Collapses.push($(this).attr('href'));
                }
                }
                console.log(Collapses)
                localStorage.setItem('collapsedItems', JSON.stringify(Collapses));
            })  
            
            $('.subParameter-collapse').on('click', function(event) {
                if(Collapses.includes($(this).attr('href'))){
                    Collapses[3] = ''
                    console.log(Collapses)
                }
                else {
                    if(Collapses.length > 3){
                    Collapses.splice(3, 1, $(this).attr('href'));
                }
                else {
                    console.log($(this).attr('href'))
                    Collapses.push($(this).attr('href'));
                }
                }
                console.log(Collapses)
                localStorage.setItem('collapsedItems', JSON.stringify(Collapses));
            })            

            $('#refreshBtn').on('click', function(event) {
                localStorage.clear();
                location.reload();
            })

        });


    </script> 
